	file = {Full Text PDF:C\:\\Users\\Mathilde JOSSERAND\\Zotero\\storage\\UE2NYD53\\Mylonas et al. - 2022 - Augmenting a colour lexicon.pdf:application/pdf;Snapshot:C\:\\Users\\Mathilde JOSSERAND\\Zotero\\storage\\95ZR9MPQ\\s41599-022-01045-3.html:text/html},
