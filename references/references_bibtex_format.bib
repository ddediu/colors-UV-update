	file = {Full Text PDF:C\:\\Users\\Mathilde JOSSERAND\\Zotero\\storage\\GPWHM3QS\\Josserand et al. - 2021 - Environment and culture shape both the colour lexi.pdf:application/pdf;Snapshot:C\:\\Users\\Mathilde JOSSERAND\\Zotero\\storage\\P3S95GJH\\s41598-021-98550-3.html:text/html},
